  "dependencies": {
    "@pulumi/aws": "^5.40.0",
    "@pulumi/digitalocean": "^4.19.1",
    "@pulumi/github": "^5.9.0",
    "@pulumi/kubernetes": "^3.27.0",
    "@pulumi/postgresql": "^3.8.0",
    "@pulumi/pulumi": "^3.67.0",
    "@pulumi/random": "^4.13.0",
    "@pulumi/tls": "^4.10.0",
    "@pulumi/vault": "^5.11.0",
    "@pulumiverse/harbor": "^3.8.4",
    "dotenv": "^16.0.3"
