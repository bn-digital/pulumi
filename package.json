{
  "name": "@bn-digital/pulumi",
  "description": "Pulumi IaC modules used by BN Digital for cloud infrastructure provisioning",
  "version": "1.4.56",
  "prettier": "@bn-digital/prettier-config",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
